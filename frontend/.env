# Server
SERVER_PORT=8080
SERVER_HOST=localhost

# Active Profile (dev, production)
VITE_ACTIVE_PROFILE=dev

# WebSocket - Development
VITE_DEV_WS_PROTOCOL=ws
VITE_DEV_WS_HOST=localhost
VITE_DEV_WS_PORT=8080
VITE_DEV_WS_ENDPOINT=/ws/connect
VITE_DEV_LOGGING_LEVEL=DEBUG

# WebSocket - Production
VITE_PROD_WS_PROTOCOL=wss
VITE_PROD_WS_HOST=api.example.com
VITE_PROD_WS_PORT=443
VITE_PROD_WS_ENDPOINT=/ws/connect
VITE_PROD_LOGGING_LEVEL=ERROR

# Common Settings
WS_MAX_CONNECTIONS_PER_SECOND=4
WS_MAX_REQUESTS_PER_SECOND=4
WS_MAX_REQUESTS_PER_MINUTE=90
WS_MIN_MESSAGE_INTERVAL=1000
WS_LOG_INTERVAL=10 